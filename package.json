{
  "author": "Turo engineering",
  "description": "Turo eslint configuration for typescript",
  "type": "module",
  "dependencies": {
    "@eslint/js": "9.19.0",
    "typescript-eslint": "8.22.0",
    "@typescript-eslint/eslint-plugin": "8.22.0",
    "@typescript-eslint/parser": "8.22.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-json": "4.0.1",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-perfectionist": "4.7.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-sonarjs": "3.0.1",
    "eslint-plugin-unicorn": "56.0.1"
  },
  "devDependencies": {
    "eslint": "9.19.0",
    "jest": "29.7.0",
    "prettier": "3.4.2"
  },
  "engines": {
    "node": ">= 20"
  },
  "homepage": "https://github.com/open-turo/eslint-config-typescript#readme",
  "license": "MIT",
  "main": "index.cjs",
  "module": "index.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "default": "./index.mjs"
    },
    "./legacy": {
      "require": "./legacy.cjs"
    }
  },
  "name": "@open-turo/eslint-config-typescript",
  "scripts": {
    "build": "echo noop",
    "lint": "eslint './*.{cjs,mjs}' test/test.spec.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "peerDependencies": {
    "eslint": "^8.18.0 || ^9.0.0",
    "prettier": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/open-turo/eslint-config-typescript",
  "version": "15.0.0"
}
