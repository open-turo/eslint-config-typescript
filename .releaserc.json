{
  "branches": ["main", "next"],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/npm",
      {
        "tarballDir": "pack"
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": "pack/*.tgz"
      }
    ],
    [
      "semantic-release-github-pullrequest",
      {
        "assets": ["package.json", "README.md", "package-lock.json"],
        "baseRef": "main"
      }
    ]
  ]
}
